    "babel": "^5.1.11",
    "babel-core": "^5.1.11",
    "babel-plugin-espower": "^0.2.1",
    "babelify": "^6.0.2",
    "browserify": "^9.0.8",
    "serve": "^11.1.0",
    "watchify": "^3.1.1"
  }
}
